{
    "name": "lntn/babytracker",
    "description": "Baby Tracker Web Tool",
    "keywords": ["baby", "laravel", "website"],
    "authors": [
        {
            "name": "Le Nguyen Trung Nam",
            "email": "me@namle.info",
            "homepage": "http://www.namle.info",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "me@namle.info",
        "issues": "https://github.com/lntn/esport-website/issues",
        "wiki": "https://github.com/lntn/esport-website/wiki",
        "source": "https://github.com/lntn/esport-website",
        "docs": "https://github.com/lntn/esport-website/wiki"
    },
    "license": "GPL-3.0",
    "type": "web",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
